<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="logistics.code">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.sender">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.senderPhone">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.senderAddress">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.recipient">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.recipientPhone">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.recipientAddress">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="serviceDate">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.billId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.memberId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="logistics.timeId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
</validators>
