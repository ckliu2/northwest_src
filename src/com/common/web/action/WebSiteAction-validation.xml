<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="webSite.code">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="webSite.name">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="webSite.seoTitleId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="webSite.seoKeywordId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="webSite.seoDescId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="webSite.homeUri">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="webSite.countryId">
        <field-validator type="required">
            <message key="errors.required"></message>
        </field-validator>
    </field>
</validators>
