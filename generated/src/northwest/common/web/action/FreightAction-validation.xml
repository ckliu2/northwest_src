<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="freight.name">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="freight.contactName">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="freight.contactCellphone">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="freight.contactPhone">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
    <field name="freight.contactEmail">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"></message>
        </field-validator>
    </field>
</validators>
