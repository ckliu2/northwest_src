"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="profit.salesPrice">
        <field-validator type="int">
            <message key="errors.numericRequired"></message>
        </field-validator>
    </field>
    <field name="profit.salesCost">
        <field-validator type="int">
            <message key="errors.numericRequired"></message>
        </field-validator>
    </field>
    <field name="profit.purchase">
        <field-validator type="int">
            <message key="errors.numericRequired"></message>
        </field-validator>
    </field>
    <field name="profit.profit">
        <field-validator type="int">
            <message key="errors.numericRequired"></message>
        </field-validator>
    </field>
    <field name="profit.discount">
        <field-validator type="int">
            <message key="errors.numericRequired"></message>
        </field-validator>
    </field>
    <field name="profit.recdiscount">
        <field-validator type="int">
            <message key="errors.numericRequired"></message>
        </field-validator>
    </field>
</validators>
